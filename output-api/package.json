{
   "name": "output",
   "version": "1.0.0",
   "description": "Publication management for universities.",
   "devDependencies": {
      "@types/cookie-parser": "^1.4.5",
      "@types/multer": "^1.4.10",
      "@types/node": "^20.9.0",
      "ts-node": "^10.9.1",
      "typescript": "^5.2.2"
   },
   "dependencies": {
      "@nestjs/axios": "3.0.1",
      "@nestjs/cli": "^10.2.1",
      "@nestjs/common": "^10.2.8",
      "@nestjs/config": "^3.1.1",
      "@nestjs/core": "^10.2.8",
      "@nestjs/jwt": "^10.2.0",
      "@nestjs/passport": "^10.0.2",
      "@nestjs/platform-express": "^10.2.8",
      "@nestjs/swagger": "^7.1.15",
      "@nestjs/typeorm": "^10.0.0",
      "axios": "^1.6.1",
      "body-parser": "^1.20.2",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "cookie-parser": "^1.4.6",
      "cross-env": "^7.0.3",
      "express": "^4.18.2",
      "https": "^1.0.0",
      "moment": "^2.29.4",
      "mysql": "^2.18.1",
      "node": "^20.9.0",
      "papaparse": "^5.4.1",
      "passport": "^0.6.0",
      "passport-local": "^1.0.0",
      "pg": "^8.11.3",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^7.8.1",
      "swagger-ui-express": "^5.0.0",
      "typeorm": "^0.3.17"
   },
   "scripts": {
      "prebuild": "rimraf dist",
      "i": "npm i",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "node -v && cross-env NODE_ENV=dev nest start -c nest-cli.dev.json -p tsconfig.json",
      "start:dev": "node -v && cross-env NODE_ENV=dev nest start -c nest-cli.dev.json -p tsconfig.json --watch",
      "start:debug": "nest start -c nest-cli.dev.json -p tsconfig.json --debug --watch",
      "start:test": "node -v && cross-env NODE_ENV=test node dist/output-api/src/main --watch",
      "start:prod": "node -v && cross-env NODE_ENV=prod node dist/output-api/src/main --watch",
      "start:init": "node -v && cross-env NODE_ENV=dev nest start -c nest-cli.init.json -p tsconfig.json",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json"
   }
}
