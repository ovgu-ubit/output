{
   "name": "output",
   "version": "1.2.1",
   "description": "Publication management for universities.",
   "devDependencies": {
      "@types/cookie-parser": "^1.4.7",
      "@types/multer": "^1.4.12",
      "@types/node": "^22.7.5",
      "ts-node": "^10.9.2",
      "typescript": "^5.6.3",
      "rimraf": "^6.0.1"
   },
   "dependencies": {
      "@nestjs/axios": "3.0.3",
      "@nestjs/cli": "^10.4.5",
      "@nestjs/common": "^10.4.4",
      "@nestjs/config": "^3.2.3",
      "@nestjs/core": "^10.4.4",
      "@nestjs/jwt": "^10.2.0",
      "@nestjs/passport": "^10.0.3",
      "@nestjs/platform-express": "^10.4.4",
      "@nestjs/schedule": "^5.0.0",
      "@nestjs/swagger": "^7.4.2",
      "@nestjs/typeorm": "^10.0.2",
      "axios": "^1.7.7",
      "body-parser": "^1.20.3",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.1",
      "cookie-parser": "^1.4.7",
      "cross-env": "^7.0.3",
      "express": "^4.21.1",
      "https": "^1.0.0",
      "moment": "^2.30.1",
      "mysql": "^2.18.1",
      "node": "^20.18.0",
      "papaparse": "^5.4.1",
      "passport": "^0.7.0",
      "passport-local": "^1.0.0",
      "pg": "^8.13.0",
      "reflect-metadata": "^0.2.2",
      "rxjs": "^7.8.1",
      "swagger-ui-express": "^5.0.1",
      "typeorm": "^0.3.20",
      "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
      "xml-js": "^1.6.11"
   },
   "scripts": {
      "prebuild": "rimraf dist",
      "i": "npm i",
      "audit": "npm audit",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "node -v && cross-env NODE_ENV=dev nest start -c nest-cli.dev.json -p tsconfig.json",
      "start:dev": "node -v && cross-env NODE_ENV=dev nest start -c nest-cli.dev.json -p tsconfig.json --watch",
      "start:debug": "nest start -c nest-cli.dev.json -p tsconfig.json --debug --watch",
      "start:test": "node -v && cross-env NODE_ENV=test node dist/output-api/src/main --watch",
      "start:prod": "node -v && cross-env NODE_ENV=prod node dist/output-api/src/main --watch",
      "start:init": "node -v && cross-env NODE_ENV=dev nest start -c nest-cli.init.json -p tsconfig.json",
      "start:init_test": "node -v && cross-env NODE_ENV=test nest start -c nest-cli.init.json -p tsconfig.json",
      "start:init_prod": "node -v && cross-env NODE_ENV=prod nest start -c nest-cli.init.json -p tsconfig.json",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "typeorm:dev": "cross-env NODE_ENV=dev typeorm-ts-node-commonjs",
      "typeorm:test": "cross-env NODE_ENV=test typeorm-ts-node-commonjs",
      "typeorm:prod": "cross-env NODE_ENV=prod typeorm-ts-node-commonjs"
   }
}
