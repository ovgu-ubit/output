{
    "url_count": "https://api.crossref.org/works?",
    "url_items": "https://api.crossref.org/works?",
    "max_res": 20,
    "max_res_name": "rows",
    "request_mode": "offset",
    "offset_name": "offset",
    "offset_count": 0,
    "offset_start": 0,
    "query_search_schema": "query.affiliation=[search_tags]&query.bibliographic=[year]",
    "query_additional_params": [
        {
            "key": "sort",
            "value": "indexed"
        }
    ],
    "parallelCalls": 1,
    "get_count": "$.message.\"total-results\"",
    "get_items": "$.message.items",
    "exclusion_criteria": "$contains($.pub_type,\"posted-content\") or $contains($.pub_type,\"peer-review\")",
    "only_import_if_authors_inst": true
}